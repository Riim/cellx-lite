{
  "name": "cellx-lite",
  "version": "1.8.0",
  "description": "",
  "main": "dist/cellx.js",
  "typings": "dist/cellx.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rm -rf dist && webpack && tsc",
    "test": "rm -rf coverage && jest",
    "coveralls": "cat coverage/lcov.info | node_modules/.bin/coveralls",
    "debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Riim/cellx-lite.git"
  },
  "author": "Dmitry Vibe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Riim/cellx-lite/issues"
  },
  "homepage": "https://github.com/Riim/cellx-lite",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  },
  "dependencies": {
    "@riim/logger": "^1.0.2",
    "@riim/next-tick": "^1.1.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
